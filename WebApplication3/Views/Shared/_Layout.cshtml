<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Scripts.Render("~/bundles/modernizr")

    <!-- Favicons -->
    <link href="img/favicon.png" rel="icon">
    <link href="img/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,700,700i|Raleway:300,400,500,700,800" rel="stylesheet">

    <!-- Bootstrap CSS File -->
    @Styles.Render("~/Content/lib/bootstrap/css/bootstrap.min.css")

    <!-- Libraries CSS Files -->
    @Styles.Render("~/Content/lib/font-awesome/css/font-awesome.min.css")

    @Styles.Render("~/Content/lib/animate/animate.min.css")
    @Styles.Render("~/Content/lib/venobox/venobox.css")
    @Styles.Render("~/Content/lib/owlcarousel/assets/owl.carousel.min.css")

    <!-- Main Stylesheet File -->
    @Styles.Render("~/Content/css/style.css")
